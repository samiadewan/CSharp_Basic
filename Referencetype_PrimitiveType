using System;

namespace Structure
{
    class Program
    {
        public class MyClass
        {
            public int value;
        }

        public static void Main()
        {

            MyClass obj1 = new MyClass();
            obj1.value = 10;

            MyClass obj2 = obj1;


            obj2.value = 20;


            Console.WriteLine(obj1.value);
            Console.WriteLine(obj2.value);

            int num1 = 10;

            int num2 = num1;
            num2 = 20;

            Console.WriteLine(num1); 
            Console.WriteLine(num2); 
        }

    }
}